{
    "name": "syahmi-jalil/database-to-model",
    "description": "Laravel package to generate models and migrations from existing database structure",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Syahmi Jalil",
            "email": "syahmijalil.my@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "SyahmiJalil\\DatabaseToModel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SyahmiJalil\\DatabaseToModel\\DatabaseToModelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 